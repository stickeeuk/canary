{
    "name": "stickee/canary",
    "description": "Stickee PHP Code Style tools",
    "homepage": "https://github.com/stickeeuk/canary",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/stickeeuk/canary/issues",
        "source": "https://github.com/stickeeuk/canary"
    },
    "authors": [
        {
            "name": "Paul Tibbetts",
            "email": "paul.tibbetts@stickee.co.uk"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel-zero/framework": "^9.2",
        "nunomaduro/termwind": "^1.14",
        "stickee/larastan-config": "^2.0",
        "stickee/php-cs-fixer-config": "^2.0",
        "stickee/rector-config": "^2.0",
        "symfony/process": "^6.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.3",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["canary"]
}
